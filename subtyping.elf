%%%%% Subtyping relations %%%%%

<t : ty -> ty -> type.
%infix none 30 <t.

<t/bool  : bool <t bool.
<t/nat   : nat <t nat.
<t/unit  : unit <t unit.
<t/empty : empty <t empty.
<t/-->   : A --> B <t A' --> B'
            <- B <t B'
            <- A' <t A.
<t/inst  : inst Eff <t inst Eff.
<t/==>   : B1 ==> B2 <t B1' ==> B2'
            <- B2 <t B2'
            <- B1' <t B1.
